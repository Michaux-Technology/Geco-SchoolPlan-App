{
  "expo": {
    "name": "Geco-SchoolPlanApp",
    "slug": "geco-schoolplan-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-gecko-modern.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "package": "com.geco.schoolplanapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "scheme": "geco-schoolplan-app",
    "newArchEnabled": true,
    "developmentClient": {
      "silentLaunch": false
    },
    "extra": {
      "eas": {
        "projectId": "b9fd9bdd-847a-4621-9a6f-32ec1feacfbb"
      }
    }
  }
}
